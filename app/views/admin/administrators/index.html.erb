<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>name</th>
      <th>email</th>
    </tr>
  </thead>
  <tbody>
    <% @administrators.each do |admin| %>
      <tr>
        <td><%= admin.name %></td>
        <td><%= admin.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @administrators %>